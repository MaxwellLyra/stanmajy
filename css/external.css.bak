	text-align: center;
	margin-top: 1%;
}
	
.jsocialmediaimg { /*STYLING SOCIAL MEDIA ICONS*/
	width: 7%;
	height: 30%;
}
	.jleft {
		margin-left: 35%;
		margin-top:2%;
	}
	
#jcopyright{ /*CLASS TO EDIT COPYRIGHT TEXT*/
	color: #b5aa62; 
	font-size: 120%;
